procedure VerificaUpdate;
begin
  {
    1º - Verifica a versão do banco de dados com a versão do sistema
    2º - Se ocorrer algum erro, automaticamente o sistema corre com a atualização
    3º - Se na pesquisa por uma versao se não for encontrado corre com a atualização
    4º - Se na comparação da versão do banco com a versão do sistema, se a versão do banco for
         menor corre com a atualização.
  }
  PreparaQuery;
  with qryConsSQL do begin
    try
      sql.Add('SELECT');
      sql.Add('      MIN(CSIS_VERSION_SYSTEM) AS VERSAO');
      sql.Add(' FROM CONF_SISTEMA');
      Open;
      if not IsEmpty then begin
        if qryConsSQL.FieldByName('VERSAO').AsInteger < intVersion then begin
          UpdateSystem(FieldByName('VERSAO').AsInteger);
          VerificaUpdate;
        end;
      end else begin
        UpdateSystem(0);
        VerificaUpdate;
      end;
    except on e: exception do begin
      UpdateSystem(0);
      VerificaUpdate;
    end; end;
  end;
end;

procedure PreparaQuery;
begin
  try
    FreeAndNil(qryConsSQL);
  except
  end;
  qryConsSQL := TSQLQuery.Create(nil);
  with qryConsSQL do begin
    SQLConnection := TFuncoes(fPrincipal.funcPrincipal).Conexao;
    Close;
    SQL.Clear;
  end;
end;

procedure AtualizaVersao;
begin
  ExecSQL('UPDATE CONF_SISTEMA SET CSIS_VERSION_SYSTEM = ' + IntToStr(intVersion));
end;

